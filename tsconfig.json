{
  "compilerOptions": {
      "module": "es2015",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "allowJs": true
  },
  "include": [
      "./src/**/*",
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}